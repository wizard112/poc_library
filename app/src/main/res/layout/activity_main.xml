<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.gael.poclibtest.MainActivity"
    android:orientation="vertical">

    <Button
        android:id="@+id/btn_camera_permission"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/btn_test_camera_permission"/>

    <Button
        android:id="@+id/btn_write_file_permission"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/btn_test_write_file_permission"/>

    <Button
        android:id="@+id/btn_request_food"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/btn_test_request"/>

</LinearLayout>
